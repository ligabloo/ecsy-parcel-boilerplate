{
  "name": "ecsy-parcel-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate project for using ECSY with Parcel.",
  "main": "dist/index.html",
  "scripts": {
    "start": "parcel serve ./src/index.html --dist-dir .serve",
    "build": "parcel build ./src/index.html --public-url ./",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "ecsy": "^0.4.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "parcel": "^2.0.0-beta.1"
  },
  "keywords": [
    "ecsy",
    "parceljs",
    "parcel",
    "javascript",
    "boilerplate",
    "template"
  ],
  "author": "Matheus Ligabue da Silva <matheus.s.ligabue@gmail.com> (https://github.com/ligabloo)",
  "license": "MIT"
}
